meta {
  name: Retrieve App Log Flow counts from device
  type: http
  seq: 2
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/app/log/flow-count?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

meta {
  name: Retrieve local properties list from vManage
  type: http
  seq: 14
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/control/synced/localproperties?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

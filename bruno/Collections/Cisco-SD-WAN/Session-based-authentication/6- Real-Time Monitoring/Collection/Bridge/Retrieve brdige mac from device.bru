meta {
  name: Retrieve brdige mac from device
  type: http
  seq: 1
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/bridge/mac?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
  ~bridge-id: {{bridgeId}}
  ~if-name: {{ifName}}
  ~mac-address: {{macAddress}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

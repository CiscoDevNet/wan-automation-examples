meta {
  name: Retrieve interface error statistics
  type: http
  seq: 5
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/interface/error_stats?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
  ~vpn-id: {{vpnId}}
  ~ifname: {{ifName}}
  ~af-type: {{afType}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

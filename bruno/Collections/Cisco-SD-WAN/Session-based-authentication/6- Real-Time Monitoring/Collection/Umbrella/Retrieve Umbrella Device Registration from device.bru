meta {
  name: Retrieve Umbrella Device Registration from device
  type: http
  seq: 3
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/umbrella/device-registration?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

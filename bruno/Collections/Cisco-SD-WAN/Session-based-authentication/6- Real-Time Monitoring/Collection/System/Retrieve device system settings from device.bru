meta {
  name: Retrieve device system settings from device
  type: http
  seq: 3
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/csp/system/settings?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

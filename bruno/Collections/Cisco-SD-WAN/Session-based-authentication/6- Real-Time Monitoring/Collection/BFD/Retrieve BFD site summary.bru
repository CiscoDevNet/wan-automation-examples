meta {
  name: Retrieve BFD site summary
  type: http
  seq: 6
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/bfd/sites/summary?isCached={{isCached}}
  body: none
  auth: none
}

params:query {
  isCached: {{isCached}}
  ~vpnId: {{vpnId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

meta {
  name: Retrieve list of BFD sessions from vManage
  type: http
  seq: 3
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/bfd/synced/sessions?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
  ~system-ip: {{systemIp}}
  ~color: {{color}}
  ~local-color: {{localColor}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

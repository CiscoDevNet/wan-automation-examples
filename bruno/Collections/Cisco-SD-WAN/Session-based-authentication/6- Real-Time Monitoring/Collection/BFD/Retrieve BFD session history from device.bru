meta {
  name: Retrieve BFD session history from device
  type: http
  seq: 4
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/bfd/history?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
  ~system-ip: {{systemIp}}
  ~color: {{color}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

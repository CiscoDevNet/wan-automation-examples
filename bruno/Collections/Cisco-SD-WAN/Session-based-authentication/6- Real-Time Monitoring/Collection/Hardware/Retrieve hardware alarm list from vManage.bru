meta {
  name: Retrieve hardware alarm list from vManage
  type: http
  seq: 4
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/hardware/synced/alarms?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

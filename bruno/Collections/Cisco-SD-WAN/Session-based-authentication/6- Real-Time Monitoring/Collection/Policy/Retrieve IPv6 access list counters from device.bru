meta {
  name: Retrieve IPv6 access list counters from device
  type: http
  seq: 6
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/policy/ipv6/accesslistcounters?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

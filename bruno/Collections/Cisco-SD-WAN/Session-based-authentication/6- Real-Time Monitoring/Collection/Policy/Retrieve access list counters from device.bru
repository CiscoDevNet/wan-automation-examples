meta {
  name: Retrieve access list counters from device
  type: http
  seq: 2
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/policy/accesslistcounters?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

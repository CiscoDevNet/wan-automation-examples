meta {
  name: Retrieve NTP peer associations list from device
  type: http
  seq: 1
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/ntp/associations?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

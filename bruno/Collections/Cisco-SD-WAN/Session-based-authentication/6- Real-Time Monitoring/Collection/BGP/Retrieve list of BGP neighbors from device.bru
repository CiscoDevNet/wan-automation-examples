meta {
  name: Retrieve list of BGP neighbors from device
  type: http
  seq: 3
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/bgp/neighbors?deviceId={{deviceId}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
  ~vpn-id: {{vpnId}}
  ~peer-addr: {{peerAddr}}
  ~as: {{as}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

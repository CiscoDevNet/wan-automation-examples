meta {
  name: AAR statistics
  type: http
  seq: 1
}

get {
  url: https://{{vmanage}}:{{port}}/dataservice/device/app-route/statistics?deviceId={{deviceId}}&remote-system-ip={{remoteSystemIp}}&local-color={{localColor}}&remote-color={{remoteColor}}
  body: none
  auth: none
}

params:query {
  deviceId: {{deviceId}}
  remote-system-ip: {{remoteSystemIp}}
  local-color: {{localColor}}
  remote-color: {{remoteColor}}
}

headers {
  X-XSRF-TOKEN: {{X-XSRF-TOKEN}}
}

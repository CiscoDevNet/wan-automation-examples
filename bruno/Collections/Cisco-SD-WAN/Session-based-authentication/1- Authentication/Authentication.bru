meta {
  name: Authentication
  type: http
  seq: 1
}

post {
  url: https://{{vmanage}}:{{port}}/j_security_check
  body: formUrlEncoded
  auth: none
}

headers {
  Content-Type: application/x-www-form-urlencoded
}

body:form-urlencoded {
  j_username: {{j_username}}
  j_password: {{j_password}}
}
